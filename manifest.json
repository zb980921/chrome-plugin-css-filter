{
  "name": "chrome-css-filter",
  "version": "0.0.1",
  "description": "Change css property `filter` of html",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
